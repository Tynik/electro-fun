    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "html-webpack-plugin": "5.6.0",
    "netlify-cli": "17.34.3",
    "prettier": "3.3.3",
    "sass": "1.77.8",
    "sass-loader": "14.2.1",
    "stripe": "16.8.0",
    "style-loader": "4.0.0",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
