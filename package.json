{
  "name": "smartstream-electronics",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "sitemap": "tsc -p tsconfig.generators.json && tsc-alias -p tsconfig.generators.json && node generators/dist/generators/sitemap-generator.js",
    "products": "tsc -p tsconfig.generators.json && tsc-alias -p tsconfig.generators.json && node generators/dist/generators/products-generator.js",
    "build": "NODE_ENV=production webpack",
    "deploy": "yarn run sitemap && yarn run products && yarn run build",
    "start": "LOCAL_ENV=true NETLIFY_SERVER=http://localhost:9999 NODE_ENV=development webpack serve",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@fontsource/roboto": "5.0.12",
    "@mui/icons-material": "5.15.15",
    "@mui/material": "5.15.15",
    "@tynik/react-honey-form": "19.0.0",
    "lodash.debounce": "4.0.8",
    "lodash.sortby": "4.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-responsive-carousel": "3.2.23",
    "react-router-dom": "6.22.3",
    "react-swipeable-views": "0.14.0",
    "react-query": "3.39.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.24.3",
    "@babel/preset-env": "7.24.4",
    "@babel/preset-typescript": "7.24.1",
    "@netlify/functions": "2.6.0",
    "@types/jest": "29.5.12",
    "@types/lodash.debounce": "4.0.9",
    "@types/lodash.sortby": "4.7.9",
    "@types/node": "20.12.7",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "html-webpack-plugin": "5.6.0",
    "netlify-cli": "17.22.1",
    "prettier": "3.2.5",
    "sass": "1.75.0",
    "sass-loader": "14.2.1",
    "stripe": "15.2.0",
    "style-loader": "4.0.0",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.8",
    "typescript": "5.4.5",
