    {
      "categoryId": 9,
      "lang": "ru",
      "title": "LoLin NodeMCU ESP8266 4 Мб v3",
      "subtitle": "DevKit",
      "seo": {
        "description": "LoLin NodeMCU ESP8266 V3",
        "keywords": "nodemcu, esp8266, esp-12e, devkit, arduino, wifi, iot, 4мб",
